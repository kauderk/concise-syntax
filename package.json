{
  "name": "concise-syntax",
  "displayName": "Concise Syntax",
  "description": "Hide unnecessary syntax or markup from programming languages",
  "version": "0.0.1",
  "publisher": "kauderk",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./dist/extension",
  "scripts": {
    "build": "tsc",
    "vscode:uninstall": "node ./dist/uninstall"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.reload",
        "title": "Concise Syntax: Reload"
      }
    ],
    "themes": [
      {
        "label": "Concise Syntax",
        "uiTheme": "vs-dark",
        "path": "./themes/tokens.json"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/injection.json",
        "scopeName": "source.concise",
        "injectTo": [
          "source.js",
          "source.ts",
          "source.tsx",
          "source.jsx",
          "source.abc"
        ]
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "vscode": "^1.1.37"
  }
}
