{
  "name": "Zen Syntax Tokens",
  "tokenColors": [
    {
      "name": "Contrast plain text",
      "scope": ["meta.jsx.children.tsx"],
      "settings": {
        "foreground": "#B59E7A"
      }
    },
    {
      "name": "Zen Syntax Tokens Targets",
      "scope": [
        "punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.end.tsx",
        "punctuation.definition.string.begin.tsx",
        "punctuation.definition.string.end.tsx",
        "punctuation.section.embedded.begin.tsx",
        "punctuation.section.embedded.end.tsx",
        "punctuation.terminator.statement.tsx",
        "zen.redundant-syntax"
      ],
      "settings": {
        "foreground": "#00b51b00"
      }
    }
  ],
  "colors": {
    "editorBracketHighlight.foreground1": "#00b51b00",
    "editorBracketHighlight.foreground2": "#00b51b00",
    "editorBracketHighlight.foreground3": "#00b51b00",
    "editorBracketHighlight.unexpectedBracket.foreground": "#00b51b00"
  }
}
